<?xml version="1.0"?>
<block>
  <name>Tag Decoder</name>
  <key>rfid_tag_decoder</key>
  <category>rfid</category>
  <import>import rfid</import>
  <make>rfid.tag_decoder($sample_rate)</make>

  <param>
    <name>Sample Rate</name>
    <key>sample_rate</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  <source>
    <name>out</name>
    <type>float</type>
  </source>

  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
